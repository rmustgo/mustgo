{
    "manifest_version": 3,
    "name": "Njeren to Njeri",
    "version": "1.0",
    "description": "Replaces 'njeren wa babab' with 'njeri wa milk'.",
    "permissions": ["scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }
  